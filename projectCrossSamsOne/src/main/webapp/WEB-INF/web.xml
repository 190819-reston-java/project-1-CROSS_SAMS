<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    
<display-name>Archetype Created Web Application</display-name>
<servlet>
	<servlet-name>Login</servlet-name>
	<display-name>Login</display-name>
	<description></description>
	<servlet-class>servlet.Login</servlet-class>
</servlet>
<servlet>
	<servlet-name>Homepage</servlet-name>
	<display-name>Homepage</display-name>
	<description></description>
	<servlet-class>servlet.Homepage</servlet-class>
</servlet>
<servlet>
	<servlet-name>reimbursementServlet</servlet-name>
	<display-name>reimbursementServlet</display-name>
	<description></description>
	<servlet-class>servlet.reimbursementServlet</servlet-class>
</servlet>
<servlet>
	<servlet-name>Logout</servlet-name>
	<display-name>Logout</display-name>
	<description></description>
	<servlet-class>servlet.Logout</servlet-class>
</servlet>
<servlet>
	<servlet-name>Employees</servlet-name>
	<display-name>Employees</display-name>
	<description></description>
	<servlet-class>servlet.Employees</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/Login</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Homepage</servlet-name>
	<url-pattern>/Homepage</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>reimbursementServlet</servlet-name>
	<url-pattern>/reimbursementServlet</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Logout</servlet-name>
	<url-pattern>/Logout</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Employees</servlet-name>
	<url-pattern>/Employees</url-pattern>
</servlet-mapping>
</web-app>
