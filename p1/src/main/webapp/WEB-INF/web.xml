<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    
<display-name>Archetype Created Web Application</display-name>
<servlet>
	<servlet-name>Login</servlet-name>
	<display-name>Login</display-name>
	<description></description>
	<servlet-class>servlet.Login</servlet-class>
</servlet>
<servlet>
	<servlet-name>Homepage</servlet-name>
	<display-name>Homepage</display-name>
	<description></description>
	<servlet-class>servlet.Homepage</servlet-class>
</servlet>
<servlet>
	<servlet-name>Reimbursement</servlet-name>
	<display-name>Reimbursement</display-name>
	<description></description>
	<servlet-class>servlet.Reimbursement</servlet-class>
</servlet>


<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/Login</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>Homepage</servlet-name>
	<url-pattern>/Homepage</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>Reimbursement</servlet-name>
	<url-pattern>/Reimbursement</url-pattern>
</servlet-mapping>
</web-app>
